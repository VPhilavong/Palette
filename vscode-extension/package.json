{
  "name": "code-palette-diagnostic",
  "displayName": "Palette AI (Diagnostic)",
  "description": "Diagnostic version to test basic VS Code extension functionality",
  "version": "0.1.0-diagnostic",
  "publisher": "sail-project",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension-diagnostic.js",
  "keywords": [
    "diagnostic",
    "test"
  ],
  "activationEvents": [
    "onCommand:palette.diagnostic",
    "onCommand:palette.openDiagnostic"
  ],
  "contributes": {
    "commands": [
      {
        "command": "palette.diagnostic",
        "title": "ðŸ©º Palette: Test Basic Function",
        "category": "Palette"
      },
      {
        "command": "palette.openDiagnostic",
        "title": "ðŸ©º Palette: Open Diagnostic Webview",
        "category": "Palette"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "palette.diagnostic"
        },
        {
          "command": "palette.openDiagnostic"
        }
      ]
    },
    "configuration": {
      "title": "Palette",
      "properties": {
        "palette.openaiApiKey": {
          "type": "string",
          "default": "",
          "description": "OpenAI API Key for Palette AI generation"
        },
        "palette.defaultModel": {
          "type": "string",
          "default": "gpt-4.1-2025-04-14",
          "description": "Default AI model to use"
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p .",
    "watch": "tsc -w -p ."
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.66",
    "ai": "^4.0.0",
    "dotenv": "^17.2.1"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.9.4"
  }
}
