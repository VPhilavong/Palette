{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Run Code Palette Extension",
        "type": "extensionHost",
        "request": "launch",
        "runtimeExecutable": "${execPath}",
        "args": [
          "--extensionDevelopmentPath=${workspaceFolder}",
          "--disable-extensions"
        ],
        "outFiles": [
          "${workspaceFolder}/vscode-extension/out/**/*.js"
        ],
        "preLaunchTask": "npm: compile"
      }
    ]
  }
  